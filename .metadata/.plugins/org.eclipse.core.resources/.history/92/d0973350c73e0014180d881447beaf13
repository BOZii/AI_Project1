
public class Man {
	private int weight; 
	private int height;
	
	Man(int h, int w){
		this.weight = w; 
		this.height = h;
	}
	
	public int getHeight(){
		return height;
	}

	public int getWeight(){
		return height;
	}
	public String toString(){
		return "Height: " + height + " Weight: " + weight;
	}


}
