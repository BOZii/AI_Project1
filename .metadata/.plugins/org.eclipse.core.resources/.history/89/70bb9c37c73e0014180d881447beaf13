
public class Woman {
	private int weight; 
	private int height;
	
	Woman(int w, int h){
		this.weight = w; 
		this.height = h;
	}
	public int getHeight(){
		return height;
	}

	public int getWeight(){
		return height;
	}
	public String toString(){
		return "Height: " + height + " Weight: " + weight;
	}

}
